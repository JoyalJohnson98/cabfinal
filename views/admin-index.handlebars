<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>

   

    <div class="container">

       

        <pre></pre>
        <table class="table table-striped mt-4">

            <tr>
                <th>Id</th>
                <th>FirstName</th>
                <th>LastName</th>
                <th>DOB</th>
                <th>Mobile</th>
                <th>Email</th>
                <th>Password</th>

            </tr>
            {{#each data}}
            <tr>
                <td>{{this.dataValues.id}}</td>
                <td>{{this.dataValues.firstname}}</td>
                <td>{{this.dataValues.lastname}}</td>
                <td>{{this.dataValues.dob}}</td>
                <td>{{this.dataValues.mobile}}</td>
                <td>{{this.dataValues.email}}</td>
                <td>{{this.dataValues.Password}}</td>

                <td>
                    <a href="/adminupdate/ {{this.dataValues.id}}">Update</a>
                    <a href="/admindelete/{{this.dataValues.id}}" class="text-danger"
                        onclick="return confirm('Are you sure you want to delete this')">Delete</a>
                </td>
            </tr>
            {{/each}}
        </table>
        <p class="text-end">You are Signin in as: 
            <b>{{identity.firstName}} {{identity.lastName}}</b>
        </p>
    </div>

</body>

</html>